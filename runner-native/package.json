{
  "name": "@bluelibs/runner-native",
  "version": "0.1.0",
  "description": "High-performance HTTP tunnel using Rust + Node.js native addon",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "runner-native",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-unknown-linux-gnu",
        "aarch64-unknown-linux-gnu",
        "x86_64-apple-darwin",
        "aarch64-apple-darwin"
      ]
    }
  },
  "scripts": {
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "node test.js",
    "example": "node example.js",
    "version": "napi version"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bluelibs/runner"
  },
  "keywords": [
    "rust",
    "napi",
    "http",
    "tunnel",
    "performance",
    "bluelibs"
  ]
}
