{
  "name": "@bluelibs/runner",
  "version": "5.4.0",
  "description": "BlueLibs Runner",
  "sideEffects": false,
  "main": "dist/universal/index.cjs",
  "module": "dist/universal/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "browser": {
        "import": "./dist/browser/index.mjs",
        "require": "./dist/browser/index.cjs",
        "default": "./dist/browser/index.mjs"
      },
      "node": {
        "types": "./dist/types/node/index.d.ts",
        "import": "./dist/node/node.mjs",
        "require": "./dist/node/node.cjs"
      },
      "import": "./dist/universal/index.mjs",
      "require": "./dist/universal/index.cjs",
      "default": "./dist/universal/index.mjs"
    },
    "./node": {
      "types": "./dist/types/node/index.d.ts",
      "import": "./dist/node/node.mjs",
      "require": "./dist/node/node.cjs",
      "default": "./dist/node/node.mjs"
    },
    "./edge": {
      "import": "./dist/edge/index.mjs",
      "default": "./dist/edge/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "node": [
        "dist/types/node/index.d.ts"
      ],
      "node/*": [
        "dist/types/node/*"
      ],
      "*": [
        "dist/types/index.d.ts"
      ]
    }
  },
  "homepage": "https://runner.bluelibs.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/bluelibs/runner"
  },
  "bugs": {
    "url": "https://github.com/bluelibs/runner/issues"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/bluelibs"
    }
  ],
  "keywords": [
    "dependency-injection",
    "framework",
    "typescript",
    "events",
    "middleware",
    "tasks",
    "nodejs",
    "serverless",
    "architecture"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup --config config/tsup/tsup.config.ts --sourcemap && npm run build:types",
    "build:types": "tsc -p config/ts/tsconfig.types.json",
    "build:dashboard": "node ./scripts/build-dashboard.mjs",
    "build:dashboard:ci": "node ./scripts/build-dashboard.mjs --install",
    "clean": "rm -rf dist",
    "watch": "tsup --config config/tsup/tsup.config.ts --watch",
    "test": "node ./scripts/run-jest-watchdog.mjs -- --config config/jest/jest.config.js --maxWorkers=50% --verbose=false",
    "test:serial": "node ./scripts/run-jest-watchdog.mjs -- --config config/jest/jest.config.js --verbose --runInBand",
    "test:dev": "jest --config config/jest/jest.config.js --verbose --watch",
    "coverage": "jest --config config/jest/jest.config.js --verbose --coverage",
    "coverage:ai": "node ./scripts/run-coverage-ai.mjs",
    "verbose:coverage": "jest --config config/jest/jest.config.js --verbose --coverage --verbose=false --silent --noStackTrace --bail=1",
    "test:clean": "jest --config config/jest/jest.config.js --clearCache",
    "testonly": "npm test",
    "test:ci": "node ./scripts/run-jest-watchdog.mjs -- --config config/jest/jest.config.js --verbose --coverage --ci --maxWorkers=2 --reporters=default --reporters=jest-junit",
    "test:security": "jest --config config/jest/jest.config.js --verbose --runInBand --testPathPattern=src/__tests__/security",
    "typecheck": "tsc -p config/ts/tsconfig.json --noEmit",
    "lint": "eslint --config config/eslint/eslint.config.mjs ./src",
    "lint:fix": "eslint --config config/eslint/eslint.config.mjs ./src --fix",
    "audit": "npm audit --audit-level=moderate",
    "audit:prod": "npm audit --omit=dev --audit-level=high",
    "audit:full": "npm audit --audit-level=moderate",
    "qa": "npm run coverage:ai && npm run lint:fix && npm run typecheck && npm run build",
    "prepublishOnly": "npm run clean && npm run build && npm run build:dashboard",
    "postbuild": "npm run guide:compose",
    "typedoc": "typedoc --options config/typedoc/typedoc.json",
    "benchmark": "jest --config config/jest/jest.bench.config.js --verbose --runInBand",
    "benchmark:json": "BENCHMARK_OUTPUT=./config/benchmarks/benchmark-results.json NODE_OPTIONS=--expose-gc npm run benchmark",
    "benchmark:compare": "node ./scripts/compare-benchmarks.mjs ./config/benchmarks/baseline.json ./config/benchmarks/benchmark-results.json ./config/benchmarks/benchmarks.config.json",
    "guide:compose": "node ./scripts/compose-guide.mjs"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/amqplib": "^0.10.7",
    "@types/benchmark": "^2.1.5",
    "@types/busboy": "^1.5.4",
    "@types/express": "^5.0.3",
    "@types/jest": "^27.0.0",
    "@types/node": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "8.56.0",
    "@typescript-eslint/parser": "8.56.0",
    "benchmark": "^2.1.4",
    "busboy": "^1.6.0",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.15.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-unused-imports": "^4.4.1",
    "express": "^5.2.1",
    "globals": "^16.5.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^30.1.2",
    "jest-junit": "^10.0.0",
    "prettier": "^3.7.4",
    "reflect-metadata": "^0.2.2",
    "source-map-support": "^0.5.13",
    "tailwindcss": "^4.1.12",
    "ts-jest": "^29.0.0",
    "tsup": "^8.0.0",
    "typedoc": "^0.28.17",
    "typedoc-material-theme": "^1.4.1",
    "typedoc-plugin-pages": "^1.1.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "zod": "^4.0.17"
  },
  "typings": "dist/types/index.d.ts",
  "typescript": {
    "definition": "dist/types/index.d.ts"
  },
  "files": [
    "dist",
    "node.js",
    "node.d.ts",
    "index.d.ts",
    "node",
    "README.md",
    "readmes/AI.md",
    "LICENSE.md"
  ],
  "license": "MIT",
  "dependencies": {
    "cron-parser": "^5.4.0",
    "lru-cache": "^11.1.0"
  },
  "optionalDependencies": {
    "amqplib": "^0.10.9",
    "busboy": "^1.6.0",
    "ioredis": "^5.7.0"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "singleQuote": false,
    "endOfLine": "auto",
    "overrides": [
      {
        "files": [
          "*.ts"
        ],
        "options": {
          "parser": "babel-ts"
        }
      }
    ]
  }
}
