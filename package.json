{
  "name": "@bluelibs/runner",
  "version": "4.8.6",
  "description": "BlueLibs Runner",
  "sideEffects": false,
  "main": "dist/universal/index.cjs",
  "module": "dist/universal/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": {
        "import": "./dist/browser/index.mjs",
        "require": "./dist/browser/index.cjs",
        "default": "./dist/browser/index.mjs"
      },
      "node": {
        "types": "./dist/node/node.d.ts",
        "import": "./dist/node/node.mjs",
        "require": "./dist/node/node.cjs"
      },
      "import": "./dist/universal/index.mjs",
      "require": "./dist/universal/index.cjs",
      "default": "./dist/universal/index.mjs"
    },
    "./node": {
      "types": "./dist/node/node.d.ts",
      "import": "./dist/node/node.mjs",
      "require": "./dist/node/node.cjs",
      "default": "./dist/node/node.mjs"
    },
    "./edge": {
      "import": "./dist/edge/index.mjs",
      "default": "./dist/edge/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "node": [
        "dist/node/node.d.ts"
      ],
      "node/*": [
        "dist/node/*.d.ts"
      ],
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "homepage": "https://runner.bluelibs.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/bluelibs/runner"
  },
  "bugs": {
    "url": "https://github.com/bluelibs/runner/issues"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/bluelibs"
    }
  ],
  "keywords": [
    "dependency-injection",
    "framework",
    "typescript",
    "events",
    "middleware",
    "tasks",
    "nodejs",
    "serverless",
    "architecture"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc -p tsconfig.declarations.json && tsup --config tsup.config.ts --sourcemap",
    "clean": "rm -rf dist",
    "watch": "tsup --config tsup.config.ts --watch",
    "test": "jest --verbose --runInBand",
    "test:dev": "jest --verbose --watch",
    "coverage": "jest --verbose --coverage",
    "coverage:ai": "node ./scripts/run-coverage-ai.mjs",
    "verbose:coverage": "jest --verbose --coverage --verbose=false --silent --noStackTrace --bail=1",
    "test:clean": "jest --clearCache",
    "testonly": "npm test",
    "test:ci": "jest --verbose --coverage --ci --maxWorkers=2 --reporters=default --reporters=jest-junit",
    "test:security": "jest --verbose --runInBand --testPathPattern=src/__tests__/security",
    "prepublishOnly": "npm run clean && npm run build",
    "typedoc": "typedoc --options typedoc.json",
    "benchmark": "jest --config jest.bench.config.js --verbose --runInBand",
    "benchmark:json": "BENCHMARK_OUTPUT=./benchmark-results.json NODE_OPTIONS=--expose-gc npm run benchmark",
    "benchmark:compare": "node ./scripts/compare-benchmarks.mjs ./baseline.json ./benchmark-results.json ./benchmarks.config.json"
  },
  "devDependencies": {
    "@types/benchmark": "^2.1.5",
    "@types/busboy": "^1.5.4",
    "@types/express": "^5.0.3",
    "@types/graphql": "^0.11.3",
    "@types/jest": "^27.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "8.39.0",
    "@typescript-eslint/parser": "8.39.0",
    "benchmark": "^2.1.4",
    "busboy": "^1.6.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-prettier": "3.1.1",
    "express": "^5.1.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^30.1.2",
    "jest-junit": "^10.0.0",
    "prettier": "^2.0.5",
    "reflect-metadata": "^0.2.2",
    "source-map-support": "^0.5.13",
    "tailwindcss": "^4.1.12",
    "ts-jest": "^29.0.0",
    "tsup": "^8.0.0",
    "typedoc": "^0.26.7",
    "typedoc-material-theme": "^1.1.0",
    "typedoc-plugin-pages": "^1.1.0",
    "typescript": "^5.6.2",
    "zod": "^4.0.17"
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "files": [
    "dist",
    "node.js",
    "node.d.ts",
    "index.d.ts",
    "node",
    "README.md",
    "AI.md",
    "LICENSE.md"
  ],
  "license": "MIT",
  "dependencies": {
    "@bluelibs/ejson": "^1.5.0",
    "lru-cache": "^11.1.0"
  },
  "optionalDependencies": {
    "busboy": "^1.6.0"
  }
}
