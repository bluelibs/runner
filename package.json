{
  "name": "@bluelibs/runner",
  "version": "4.2.2",
  "description": "BlueLibs Runner",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bluelibs/bluelibs"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "rm -rf dist",
    "watch": "tsc -w",
    "test": "jest --verbose --runInBand",
    "test:dev": "jest --verbose --watch",
    "coverage": "jest --verbose --coverage",
    "test:clean": "jest --clearCache",
    "testonly": "npm test",
    "test:ci": "jest --verbose --coverage --ci --maxWorkers=2 --reporters=default --reporters=jest-junit",
    "prepublishOnly": "npm run clean && npm run build",
    "typedoc": "typedoc --options typedoc.json",
    "benchmark": "jest --config jest.bench.config.js --verbose"
  },
  "devDependencies": {
    "@types/benchmark": "^2.1.5",
    "@types/express": "^5.0.3",
    "@types/graphql": "^0.11.3",
    "@types/jest": "^27.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "8.39.0",
    "@typescript-eslint/parser": "8.39.0",
    "benchmark": "^2.1.4",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-prettier": "3.1.1",
    "express": "^5.1.0",
    "jest": "^29.0.0",
    "jest-junit": "^10.0.0",
    "prettier": "^2.0.5",
    "reflect-metadata": "^0.2.2",
    "source-map-support": "^0.5.13",
    "ts-jest": "^29.0.0",
    "typedoc": "^0.26.7",
    "typedoc-material-theme": "^1.1.0",
    "typedoc-plugin-pages": "^1.1.0",
    "typescript": "^5.6.2",
    "zod": "^4.0.17"
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "files": [
    "dist",
    "README.md",
    "AI.md",
    "LICENSE.md"
  ],
  "license": "MIT",
  "dependencies": {
    "lru-cache": "^11.1.0"
  }
}
